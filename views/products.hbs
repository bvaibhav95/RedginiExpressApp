  <body id="cake_product_page">
        {{>header}}
        {{>comingSoonModal}}
        <div id="offerCarousal" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <div class="row">
                        <div class="col-md-3 col-sm-4 col-6">
                            <img class="d-block w-100" src="/images/buy3get1.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6">
                            <img class="d-block w-100" src="/images/freehomedelivery.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6 d-none d-sm-block">
                            <img class="d-block w-100" src="/images/midnightdelivery.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6 d-none d-md-block">
                            <img class="d-block w-100" src="/images/rumcake.jpg" alt="First slide">
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <div class="row">
                        <div class="col-md-3 col-sm-4 col-6">
                            <img class="d-block w-100" src="/images/healthywheat.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6">
                            <img class="d-block w-100" src="/images/vmcelebration.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6 d-none d-sm-block">
                            <img class="d-block w-100" src="/images/rumcake.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6 d-none d-md-block">
                            <img class="d-block w-100" src="/images/midnightdelivery.jpg" alt="First slide">
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <div class="row">
                        <div class="col-md-3 col-sm-4 col-6">
                            <img class="d-block w-100" src="/images/prakashbakery.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6">
                            <img class="d-block w-100" src="/images/freehomedelivery.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6 d-none d-sm-block">
                            <img class="d-block w-100" src="/images/toccata.jpg" alt="First slide">
                        </div>
                        <div class="col-md-3 col-sm-4 col-6 d-none d-md-block">
                            <img class="d-block w-100" src="/images/sugarfreecake.jpg" alt="First slide">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
        <div class="container-fluid">
            <div class="row justify-content-start">
                <div class="col-md-3 col-sm-12 col-xs-12">
                    <h4 id="pdtFilter">Choose Cakes / other bakery products...</h4>
                    <div class="container">
                        <div class="row justify-content-center" id="chooseCakeVsBake">
                            <form action="/products/select-category" method="POST">
                                <input type="hidden" name="_csrf" value="{{csrfToken}}">
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <label class="input-group-text" for="choosePdtCategory">Product</label>
                                    </div>
                                    <select class="custom-select" id="choosePdtCategory" name="choosePdtCategory">
                                        <option value="cake">Cakes</option>
                                        <option value="bake">Bakery products</option>
                                    </select>
                                </div> 
                                <button style="width : 100%; margin-left:0px; margin-top:10px;" id="selectCategoryBtn" type="submit" class="btn-sm btn customColorBtn"><b>Go now !</b></button>
                            </form>
                        </div>
                    </div>
                    <hr>
                    <h4 id="pdtFilter">Choose from your favourite place...</h4>
                    <hr>
                    {{>pdtFilter}}
                </div>
                <div class="col-md-9 col-sm-12 col-xs-12">
                    <p id="filterResults">Showing for - Nashik&nbsp;<i class="fas fa-arrow-right"></i>&nbsp;{{brandName}}&nbsp;<i class="fas fa-arrow-right"></i>&nbsp;{{categoryName}}</p>
                    {{#if pdtTypeFlag}}
                        {{>bakeryPdts}}
                    {{else}}
                        {{>cakePage}}
                    {{/if}}
                </div>
            </div>
        </div>

        <div class="container" id="famousShopBlock">
            <h4 id="zomatoSideWidget2Header">Famous cake shops in Nashik!</h4>
            <hr>
            <div class="row justify-content-center">
                <div class="col-md-2 col-sm-4 col-6">
                    <div class="view overlay famousShopPicsBlock">
                        <img class="famousShopPics" src="/images/thebakestudio.jpg" class="img-fluid " alt="zoom">
                        <div class="mask flex-center waves-effect waves-light rgba-black-light rgba-black-light">
                            <p class="white-text famousShopNames"><b>Bake studio</b></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-2 col-sm-4 col-6">
                    <div class="view overlay famousShopPicsBlock">
                        <img class="famousShopPics" src="/images/pnr.png" alt="zoom">
                        <div class="mask flex-center waves-effect waves-light rgba-black-light">
                            <p class="white-text famousShopNames"><b>PnR cake</b></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-2 col-sm-4 col-6">
                    <div class="view overlay famousShopPicsBlock">
                        <img class="famousShopPics" src="/images/bakersbounty.jpg" class="img-fluid " alt="zoom">
                        <div class="mask flex-center waves-effect waves-light rgba-black-light">
                            <p class="white-text famousShopNames"><b>Bakers bounty</b></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-2 col-sm-4 col-6">
                    <div class="view overlay famousShopPicsBlock">
                        <img class="famousShopPics" src="/images/bakerslounge.jpg" class="img-fluid " alt="zoom">
                        <div class="mask flex-center waves-effect waves-light rgba-black-light">
                            <p class="white-text famousShopNames"><b>Bakers lounge</b></p>
                        </div>
                    </div>
                </div>
                <div class="col-md-2 col-sm-4 col-6">
                    <div class="view overlay famousShopPicsBlock">
                        <img class="famousShopPics" src="/images/jahangirdar.jpg" class="img-fluid " alt="zoom">
                        <div class="mask flex-center waves-effect waves-light rgba-black-light">
                            <p class="white-text famousShopNames"><b>Jahagirdar</b></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{>footer}}
        {{>floatingAction}}
        {{>filterModal}}
